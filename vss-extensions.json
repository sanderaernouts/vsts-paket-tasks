{
    "manifestVersion": 1,
    "id": "paket-buildtasks",
    "name": "Paket CLI tasks",
    "version": "0.0.0",
    "publisher": "",
    "public" : false,
    "repository": {
        "type": "git",
        "uri": "https://github.com/sanderaernouts/vsts-paket-tasks"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Tasks using Paket as an alternative package manager inside your builds.",
    "categories": [
        "Build and release"
    ],
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "tags": [
    ],
    "links": {
        "home": {
            "uri": "https://github.com/sanderaernouts/vsts-paket-tasks"
        },
        "repository": {
            "uri": "https://github.com/sanderaernouts/vsts-paket-tasks"
        },
        "issues": {
            "uri": "https://github.com/sanderaernouts/vsts-paket-tasks/issues"
        }
    },
    "branding": {
        "color": "rgb(248,153,56)",
        "theme": "light"
    },
    "icons": {
        "default": "images/extension-icon.png"        
    },
    "files": [
        {
            "path": "tasks/paketcli"
        },
        {
            "path": "tasks/paketinstaller"
        }
    ],
    "contributions": [
        {
            "id": "paket-cli",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/paketcli"
            }
        },
        {
            "id": "paket-installer",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/paketinstaller"
            }
        }
    ]
}